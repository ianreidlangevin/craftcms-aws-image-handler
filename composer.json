{
   "name": "ianreid/aws-image-handler-urls",
   "description": "Create AWS Serverless Image Handler Urls",
   "type": "craft-plugin",
   "version": "1.0-beta.1",
   "keywords": [
      "craft",
      "cms",
      "aws",
      "image handler",
      "serverless",
      "assets",
      "craftcms",
      "craft-plugin"
   ],
   "support": {
      "docs": "https://www.reidlangevin.com",
      "issues": "https://www.reidlangevin.com"
   },
   "license": "MIT",
   "authors": [
      {
         "name": "Ian Reid Langevin",
         "homepage": "https://www.reidlangevin.com"
      }
   ],
   "require": {
      "craftcms/cms": "^3.0|^4.0",
      "craftcms/aws-s3": "^2.0"
   },
   "autoload": {
      "psr-4": {
         "ianreid\\awsimagehandlerurls\\": "src/"
      }
   },
   "extra": {
      "name": "AWS Image Handler Urls",
      "handle": "aws-image-handler-urls",
      "developer": "Ian Reid Langevin",
      "developerUrl": "https://www.reidlangevin.com",
      "documentationUrl": "https://www.reidlangevin.com",
      "changelogUrl": "https://www.reidlangevin.com",
      "class": "ianreid\\awsimagehandlerurls\\AwsImageHandlerUrls"
   },
   "require-dev": {
      "craftcms/rector": "dev-main"
   },
   "config": {
      "allow-plugins": {
         "yiisoft/yii2-composer": true,
         "craftcms/plugin-installer": true
      }
   }
}
